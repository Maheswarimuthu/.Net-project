class Program
    {
        static void Main(string[] args)
        {
            int a, b, temp;
            Console.WriteLine("Enter value for a");
            a = int.Parse(Console.ReadLine());
            Console.WriteLine("Enter value for b");
            b = int.Parse(Console.ReadLine());
            temp = a;
            a = b;
            b = temp;
            Console.WriteLine("value of a after swapping");
        }
    }
